{
    "name": "klaviyo/php-sdk",
    "description": "Klaviyo PHP SDK",
    "keywords": ["klaviyo", "sdk"],
    "type": "library",
    "homepage": "http://github.com/klaviyo",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Klaviyo",
            "homepage": "https://github.com/klaviyo"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Klaviyo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Klaviyo\\": "tests/"
        }
    }
}
